@font-face {
  font-family: 'MinSans';
  font-weight: 900;
  font-display: swap;
  src: local('MinSans-Black'),
  url('/FanFixiv-user-page/fonts/MinSans-Black.woff2') format('woff2'),
  url('/FanFixiv-user-page/fonts/MinSans-Black.woff') format('woff'),
  url('/FanFixiv-user-page/fonts/MinSans-Black.otf') format('opentype');
}

